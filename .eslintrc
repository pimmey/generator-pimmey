{
  "rules": {
    "indent": [
      2,
      4
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "space-before-function-paren": [
      2,
      "always"
    ],
    "no-console": [
      0
    ]
  },
  "env": {
    "node": true,
    "browser": true
  },
  "globals": {
    "$": true,
    "google": true,
    "CONFIG": true,
    "Materialize": true,
    "THREE": true,
    "SineWaves": true
  },
  "extends": "eslint:recommended"
}
